    "formula": "(Ah * Ubat * 0.85) / Pload",
    "unit": "h",
    "variables": [
      {"id": "Ah", "label": "Pojemność", "unit": "Ah", "defaultValue": "50"},
      {"id": "Ubat", "label": "Napięcie Bat.", "unit": "V", "defaultValue": "12"},
      {"id": "Pload", "label": "Moc Obciążenia", "unit": "W", "defaultValue": "100"}
    ]
  },
  {
    "id": "battery_charge",
    "title": "Czas Ładowania Akumulatora",
