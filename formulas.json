       {"id": "A", "label": "Powierzchnia", "unit": "m2", "defaultValue": "20"}
    ]
  },
  {
    "id": "pv_string_voltage",
    "title": "PV: Napięcie Stringu",
    "description": "Napięcie obwodu otwartego łańcucha paneli fotowoltaicznych.",
    "formula": "Voc * N",
    "unit": "V",
    "variables": [
      {"id": "Voc", "label": "Napięcie Voc panelu", "unit": "V", "defaultValue": "41.5"},
      {"id": "N", "label": "Liczba paneli", "unit": "szt", "defaultValue": "12"}
    ]
  },
  {
    "id": "pv_power",
    "title": "PV: Moc Instalacji",
    "description": "Łączna moc szczytowa instalacji.",
    "formula": "Pmod * N / 1000",
    "unit": "kWp",
    "variables": [
      {"id": "Pmod", "label": "Moc panelu", "unit": "Wp", "defaultValue": "450"},
      {"id": "N", "label": "Liczba paneli", "unit": "szt", "defaultValue": "10"}
    ]
  },
  {
    "id": "electric_field",
    "title": "Natężenie Pola Elektr.",
    "description": "Natężenie pola w kondensatorze płaskim.",
    "formula": "U / d",
    "unit": "V/m",
    "variables": [
      {"id": "U", "label": "Napięcie", "unit": "V", "defaultValue": "1000"},
      {"id": "d", "label": "Odległość okładzin", "unit": "m", "defaultValue": "0.01"}
    ]
  },
  {
    "id": "resonant_freq",
    "title": "Częstotliwość Rezonansowa",
    "description": "Częstotliwość rezonansu dla obwodu LC (Wzór Thomsona).",
    "formula": "1 / (2 * 3.14 * sqrt(L * C))",
    "unit": "Hz",
    "variables": [
      {"id": "L", "label": "Indukcyjność", "unit": "H", "defaultValue": "0.001"},
      {"id": "C", "label": "Pojemność", "unit": "F", "defaultValue": "0.00001"}
    ]
  },
  {
    "id": "trafo_power_s",
    "title": "Trafo: Moc Pozorna (S)",
    "description": "Moc pozorna transformatora 3-fazowego (S = √3 * U * I).",
    "formula": "1.732 * U * I",
    "unit": "VA",
    "variables": [
      {"id": "U", "label": "Napięcie Międzyfazowe", "unit": "V", "defaultValue": "400"},
      {"id": "I", "label": "Prąd", "unit": "A", "defaultValue": "100"}
    ]
  },
  {
    "id": "trafo_power_p",
    "title": "Trafo: Moc Czynna (P)",
    "description": "Moc czynna oddawana (P = S * cosφ).",
    "formula": "1.732 * U * I * PF",
    "unit": "W",
    "variables": [
      {"id": "U", "label": "Napięcie", "unit": "V", "defaultValue": "400"},
      {"id": "I", "label": "Prąd", "unit": "A", "defaultValue": "100"},
      {"id": "PF", "label": "Cos φ", "unit": "-", "defaultValue": "0.8"}
    ]
  },
  {
    "id": "trafo_power_q",
    "title": "Trafo: Moc Bierna (Q)",
    "description": "Moc bierna (Q = S * sinφ). Obliczana z cosφ.",
    "formula": "1.732 * U * I * sqrt(1 - PF^2)",
    "unit": "var",
    "variables": [
      {"id": "U", "label": "Napięcie", "unit": "V", "defaultValue": "400"},
      {"id": "I", "label": "Prąd", "unit": "A", "defaultValue": "100"},
      {"id": "PF", "label": "Cos φ", "unit": "-", "defaultValue": "0.8"}
    ]
  
