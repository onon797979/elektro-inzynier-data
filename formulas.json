[
  {
    "id": "basic_ohm",
    "title": "Prawo Ohma (Prąd)",
    "description": "Podstawowa zależność prądu od napięcia i rezystancji.",
    "formula": "U / R",
    "unit": "A",
    "variables": [
      {"id": "U", "label": "Napięcie", "unit": "V", "defaultValue": "230"},
      {"id": "R", "label": "Rezystancja", "unit": "Ohm", "defaultValue": "10"}
    ]
  },
  {
    "id": "basic_power_dc",
    "title": "Moc Prądu Stałego",
    "description": "Obliczanie mocy czynnej w obwodach DC.",
    "formula": "U * I",
    "unit": "W",
    "variables": [
      {"id": "U", "label": "Napięcie", "unit": "V", "defaultValue": "24"},
      {"id": "I", "label": "Prąd", "unit": "A", "defaultValue": "2"}
    ]
  },
  {
    "id": "ac_power_1ph",
    "title": "Moc Czynna (1-faza)",
    "description": "Moc w obwodzie jednofazowym AC z uwzględnieniem cos fi.",
    "formula": "U * I * PF",
    "unit": "W",
    "variables": [
      {"id": "U", "label": "Napięcie", "unit": "V", "defaultValue": "230"},
      {"id": "I", "label": "Prąd", "unit": "A", "defaultValue": "5"},
      {"id": "PF", "label": "Wsp. Mocy", "unit": "cos", "defaultValue": "0.9"}
    ]
  },
  {
    "id": "ac_power_3ph",
    "title": "Moc Czynna (3-fazy)",
    "description": "Moc w układzie trójfazowym symetrycznym.",
    "formula": "1.73 * U * I * PF",
    "unit": "W",
    "variables": [
      {"id": "U", "label": "Napięcie Międzyfaz.", "unit": "V", "defaultValue": "400"},
      {"id": "I", "label": "Prąd", "unit": "A", "defaultValue": "10"},
      {"id": "PF", "label": "Wsp. Mocy", "unit": "cos", "defaultValue": "0.9"}
    ]
  },
  {
    "id": "cable_vd_1ph",
    "title": "Spadek Napięcia (1-faza)",
    "description": "Spadek napięcia w woltach dla obwodu jednofazowego.",
    "formula": "(2 * L * I * PF) / (Gam * S)",
    "unit": "V",
    "variables": [
      {"id": "L", "label": "Długość", "unit": "m", "defaultValue": "50"},
      {"id": "I", "label": "Prąd", "unit": "A", "defaultValue": "16"},
      {"id": "PF", "label": "Wsp. Mocy", "unit": "cos", "defaultValue": "1.0"},
      {"id": "Gam", "label": "Konduktywność", "unit": "m/Ohm mm2", "defaultValue": "56"},
      {"id": "S", "label": "Przekrój", "unit": "mm2", "defaultValue": "2.5"}
    ]
  },
  {
    "id": "cable_vd_percent",
    "title": "Spadek Napięcia Proc (3f)",
    "description": "Procentowy spadek napięcia w obwodzie 3-fazowym.",
    "formula": "100 * 1.73 * I * L * PF / (Gam * S * U)",
    "unit": "%",
    "variables": [
      {"id": "I", "label": "Prąd Ib", "unit": "A", "defaultValue": "10"},
      {"id": "L", "label": "Długość L", "unit": "m", "defaultValue": "50"},
      {"id": "PF", "label": "Wsp. mocy", "unit": "phi", "defaultValue": "0.9"},
      {"id": "Gam", "label": "Konduktywność", "unit": "m/Ohm", "defaultValue": "56"},
      {"id": "S", "label": "Przekrój", "unit": "mm2", "defaultValue": "4"},
      {"id": "U", "label": "Napięcie Un", "unit": "V", "defaultValue": "400"}
    ]
  },
  {
    "id": "cable_weight",
    "title": "Masa Miedzi w Kablu",
    "description": "Szacunkowa waga czystej miedzi w przewodzie.",
    "formula": "S * L * 8.89 / 1000",
    "unit": "kg",
    "variables": [
        {"id": "S", "label": "Przekrój Łączny", "unit": "mm2", "defaultValue": "7.5"},
        {"id": "L", "label": "Długość", "unit": "m", "defaultValue": "100"}
    ]
  },
  {
    "id": "motor_sync_speed",
    "title": "Silnik: Prędkość Synch.",
    "description": "Prędkość obrotowa pola magnetycznego stojana.",
    "formula": "(60 * f) / p",
    "unit": "obr/min",
    "variables": [
      {"id": "f", "label": "Częstotliwość", "unit": "Hz", "defaultValue": "50"},
      {"id": "p", "label": "Liczba par biegunów", "unit": "-", "defaultValue": "2"}
    ]
  },
  {
    "id": "motor_slip",
    "title": "Silnik: Poślizg",
    "description": "Obliczanie poślizgu silnika indukcyjnego.",
    "formula": "((Ns - Nr) / Ns) * 100",
    "unit": "%",
    "variables": [
      {"id": "Ns", "label": "Prędkość Synch.", "unit": "rpm", "defaultValue": "1500"},
      {"id": "Nr", "label": "Prędkość Rzecz.", "unit": "rpm", "defaultValue": "1450"}
    ]
  },
  {
    "id": "motor_current",
    "title": "Silnik: Prąd Znamionowy",
    "description": "Szacowanie prądu silnika 3-fazowego na podstawie mocy.",
    "formula": "Pn / (1.73 * U * PF * Eff)",
    "unit": "A",
    "variables": [
      {"id": "Pn", "label": "Moc na wale", "unit": "W", "defaultValue": "5500"},
      {"id": "U", "label": "Napięcie", "unit": "V", "defaultValue": "400"},
      {"id": "PF", "label": "Cos Fi", "unit": "-", "defaultValue": "0.85"},
      {"id": "Eff", "label": "Sprawność (0-1)", "unit": "-", "defaultValue": "0.88"}
    ]
  },
  {
    "id": "motor_torque",
    "title": "Silnik: Moment Obrotowy",
    "description": "Moment na wale silnika przy danej mocy i obrotach.",
    "formula": "9550 * Pkw / n",
    "unit": "Nm",
    "variables": [
      {"id": "Pkw", "label": "Moc", "unit": "kW", "defaultValue": "5.5"},
      {"id": "n", "label": "Obroty", "unit": "rpm", "defaultValue": "1450"}
    ]
  },
  {
    "id": "energy_kwh",
    "title": "Zużycie Energii",
    "description": "Obliczanie energii zużytej przez urządzenie w czasie.",
    "formula": "(P * t) / 1000",
    "unit": "kWh",
    "variables": [
      {"id": "P", "label": "Moc", "unit": "W", "defaultValue": "2000"},
      {"id": "t", "label": "Czas pracy", "unit": "h", "defaultValue": "8"}
    ]
  },
  {
    "id": "heat_energy",
    "title": "Moc Grzałki (Prawo Joule'a)",
    "description": "Moc wydzielana na elemencie rezystancyjnym.",
    "formula": "I * I * R",
    "unit": "W",
    "variables": [
      {"id": "I", "label": "Prąd", "unit": "A", "defaultValue": "10"},
      {"id": "R", "label": "Rezystancja", "unit": "Ohm", "defaultValue": "23"}
    ]
  },
  {
    "id": "reactance_xl",
    "title": "Reaktancja Indukcyjna XL",
    "description": "Opór bierny cewki dla prądu przemiennego.",
    "formula": "2 * 3.14 * f * L",
    "unit": "Ohm",
    "variables": [
      {"id": "f", "label": "Częstotliwość", "unit": "Hz", "defaultValue": "50"},
      {"id": "L", "label": "Indukcyjność", "unit": "H", "defaultValue": "0.1"}
    ]
  },
  {
    "id": "reactance_xc",
    "title": "Reaktancja Pojemnościowa XC",
    "description": "Opór bierny kondensatora dla prądu przemiennego.",
    "formula": "1 / (2 * 3.14 * f * C)",
    "unit": "Ohm",
    "variables": [
      {"id": "f", "label": "Częstotliwość", "unit": "Hz", "defaultValue": "50"},
      {"id": "C", "label": "Pojemność", "unit": "F", "defaultValue": "0.0001"}
    ]
  },
  {
    "id": "res_parallel",
    "title": "Rezystory Równoległe (2szt)",
    "description": "Rezystancja zastępcza dwóch rezystorów połączonych równolegle.",
    "formula": "(R1 * R2) / (R1 + R2)",
    "unit": "Ohm",
    "variables": [
      {"id": "R1", "label": "Rezystor 1", "unit": "Ohm", "defaultValue": "100"},
      {"id": "R2", "label": "Rezystor 2", "unit": "Ohm", "defaultValue": "100"}
    ]
  },
  {
    "id": "voltage_divider",
    "title": "Dzielnik Napięcia",
    "description": "Napięcie wyjściowe na rezystorze R2.",
    "formula": "Uin * R2 / (R1 + R2)",
    "unit": "V",
    "variables": [
      {"id": "Uin", "label": "Napięcie Wej.", "unit": "V", "defaultValue": "12"},
      {"id": "R1", "label": "Rezystor R1", "unit": "Ohm", "defaultValue": "1000"},
      {"id": "R2", "label": "Rezystor R2 (GND)", "unit": "Ohm", "defaultValue": "2000"}
    ]
  },
  {
    "id": "led_resistor",
    "title": "Rezystor do LED",
    "description": "Dobór rezystora ograniczającego prąd diody.",
    "formula": "(Uzas - Uled) / Iled",
    "unit": "Ohm",
    "variables": [
      {"id": "Uzas", "label": "Napięcie Zasilania", "unit": "V", "defaultValue": "12"},
      {"id": "Uled", "label": "Napięcie Diody", "unit": "V", "defaultValue": "3.2"},
      {"id": "Iled", "label": "Prąd Diody", "unit": "A", "defaultValue": "0.02"}
    ]
  },
  {
    "id": "cap_energy",
    "title": "Energia Kondensatora",
    "description": "Energia zgromadzona w polu elektrycznym.",
    "formula": "0.5 * C * U * U",
    "unit": "J",
    "variables": [
      {"id": "C", "label": "Pojemność", "unit": "F", "defaultValue": "0.001"},
      {"id": "U", "label": "Napięcie", "unit": "V", "defaultValue": "400"}
    ]
  },
  {
    "id": "frequency_period",
    "title": "Częstotliwość z Okresu",
    "description": "Przeliczenie czasu trwania okresu na herce.",
    "formula": "1 / T",
    "unit": "Hz",
    "variables": [
      {"id": "T", "label": "Okres", "unit": "s", "defaultValue": "0.02"}
    ]
  },
  {
    "id": "wavelength",
    "title": "Długość Fali",
    "description": "Długość fali elektromagnetycznej w próżni.",
    "formula": "300 / f",
    "unit": "m",
    "variables": [
      {"id": "f", "label": "Częstotliwość", "unit": "MHz", "defaultValue": "100"}
    ]
  },
  {
    "id": "battery_runtime",
    "title": "Czas Pracy na Baterii",
