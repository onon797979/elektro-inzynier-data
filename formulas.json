    "formula": "(Ah / I) * 1.4",
    "unit": "h",
    "variables": [
      {"id": "Ah", "label": "Pojemność", "unit": "Ah", "defaultValue": "60"},
      {"id": "I", "label": "Prąd Ładowania", "unit": "A", "defaultValue": "6"}
    ]
  },
  {
    "id": "transformer_ratio",
    "title": "Przekładnia Transformatora",
    "description": "Stosunek napięcia pierwotnego do wtórnego.",
    "formula": "U1 / U2",
    "unit": "-",
    "variables": [
      {"id": "U1", "label": "Napięcie PRI", "unit": "V", "defaultValue": "230"},
      {"id": "U2", "label": "Napięcie SEC", "unit": "V", "defaultValue": "24"}
    ]
  },
  {
    "id": "short_circuit_loop",
    "title": "Prąd Zwarcia (Pętla)",
    "description": "Spodziewany prąd zwarcia na podstawie impedancji pętli.",
    "formula": "U / Zs",
    "unit": "A",
    "variables": [
      {"id": "U", "label": "Napięcie F-N", "unit": "V", "defaultValue": "230"},
      {"id": "Zs", "label": "Impedancja Pętli", "unit": "Ohm", "defaultValue": "0.5"}
    ]
  },
  {
    "id": "lux_calc",
    "title": "Natężenie Oświetlenia",
    "description": "Szacowanie liczby luksów (metoda uproszczona).",
